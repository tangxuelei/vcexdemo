{
	"name": "uihelper",
	"displayName": "uihelper",
	"description": "a util for code the user interface simpleness,efficient and so on!",
	"keywords": [
		"html",
		"css",
		"scss",
		"html2css",
		"html2scss"
	  ],
	"publisher": "tangxuelei",
	"version": "0.0.9",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Programming Languages",
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "uihelper.html2scss",
				"title": "html2scss",
				"category": "html2scss"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "uihelper.html2scss",
					"alt": "uihelper.html2scss",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.1.4",
		"vscode": "^1.1.25",
		"tslint": "^5.8.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"parse5": "^5.1.0"
	}
}
